	<descMeta>
		<title xml:lang="en">TURS 3578 Mould</title>

		<typeDesc>
			<objectType xlink:href="http://nomisma.org/id/mould">Mould</objectType>
			<dateRange>
				<fromDate standardDate="0001">A.D. 1</fromDate>
				<toDate standardDate="0300">A.D. 300</toDate>
			</dateRange>
			<material xlink:type="simple" xlink:href="http://nomisma.org/id/palombino_marble"
				>Palombino Marble</material>
			<obverse>
				<type>
					<description xml:lang="en">Mould for 5 circular tokens of c. 25mm, all of the
						same design: Togate male standing right holding corn-ears or purse in left
						hand. There is a central canal and two holes for nails in the top right and
						lower left corners. On the upper and lower sides of the mould are ridges
						made for/from wire.</description>
				</type>
			</obverse>
		</typeDesc>

