		<title xml:lang="en">Mitchiner 4 = Stannard Liri 29.015</title>

		<typeDesc>
			<objectType xlink:href="http://nomisma.org/id/token">Token</objectType>
			<dateRange>
				<fromDate standardDate="0041">A.D. 41</fromDate>
				<toDate standardDate="0048">A.D. 48</toDate>
			</dateRange>
			<manufacture xlink:type="simple" xlink:href="http://nomisma.org/id/cast"
