			<obverse>
				<type>
					<description xml:lang="en">Bare head of Claudius left</description>
				</type>
				<legend>TI CLAVDIVS CAE</legend>
				<persname xlink:type="simple" xlink:role="portrait"
					xlink:href="http://nomisma.org/id/claudius">Claudius</persname>
			</obverse>
			<reverse>
				<type>
					<description xml:lang="en">Venus standing left with left hand resting on the
						head of the cupid, who holds a rudder. </description>
				</type>
				<legend>COLO VEN (?)</legend>




				<persname xlink:type="simple" xlink:role="deity"
